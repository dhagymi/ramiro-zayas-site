{
	"name": "javascript-wihtout-frameworks-boilerplate",
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"backend:prod": "node --experimental-modules app.js",
		"backend:dev": "nodemon --experimental-modules app.js",
		"frontend:prod": "webpack --progress --config webpack.config.build.js",
		"frontend:dev": "webpack serve --progress --config webpack.config.development.js",
		"start:dev": "concurrently --kill-others \"yarn backend:dev\" \"yarn frontend:dev\"",
		"start:prod": "concurrently --kill-others \"yarn frontend:prod && yarn backend:prod\"",
		"build": "\"yarn frontend:prod\"",
		"start": "\"yarn backend:prod\""
	},
	"author": "Agustin Andreacchi",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.16.0",
