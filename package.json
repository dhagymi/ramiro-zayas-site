  "scripts": {
    "backend:prod": "node --experimental-modules app.js",
    "backend:dev": "nodemon --experimental-modules app.js",
    "frontend:prod": "webpack --progress --config webpack.config.build.js",
    "frontend:dev": "webpack serve --progress --config webpack.config.development.js",
    "start:dev": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start:prod": "concurrently --kill-others \"npm run frontend:prod && npm run backend:prod\"",
    "build": "webpack --progress --config webpack.config.build.js",
    "start": "node --experimental-modules --trace-warnings app.js"
  },
